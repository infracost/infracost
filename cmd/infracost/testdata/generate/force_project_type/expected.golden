version: 0.1

projects:
  - path: dup
    name: dup-dev
    terraform_var_files:
      - dev.tfvars
    skip_autodetect: true
    dependency_paths:
      - dup/**
      - dup/dev.tfvars
  - path: dup
    name: dup-prod
    terraform_var_files:
      - prod.tfvars
    skip_autodetect: true
    dependency_paths:
      - dup/**
      - dup/prod.tfvars
  - path: nondup
    name: nondup

